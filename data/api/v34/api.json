        {
          "name": "ID",
          "kind": "string"
        },
        {
          "name": "Time",
          "kind": "datetime"
        },
        {
          "name": "Emotes",
          "kind": "array",
          "element": {
            "name": "",
            "kind": "object",
            "keys": [
              {
                "name": "Name",
                "kind": "string"
              },
              {
                "name": "ID",
                "kind": "string"
              },
              {
                "name": "Count",
